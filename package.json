    "trakt.tv-images": "^5.2.0",
    "trakt.tv-queued": "^3.0.0",
    "vite": "^6.3.5",
    "vite-express": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^3.2.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
