    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "brace-expansion": "1.1.12",
    "connect-ensure-login": "^0.1.1",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.17.3",
    "jest": "^29.7.0",
    "native-promise-pool": "^3.28.0",
