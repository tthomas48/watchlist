      "brace-expansion": "1.1.12"
    },
    "fanart.tv": {
      "got": "11.8.5"
    },
    "omdbapi": {
      "got": "11.8.5"
    },
    "trakt.tv-images": {
      "got": "11.8.5"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "resolvePluginsRelativeTo": "."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
