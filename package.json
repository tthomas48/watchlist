{
  "name": "watchlist",
  "version": "1.0.0",
  "description": "Checks the availability of movies and shows from your Trakt.tv watchlist on Netflix and Prime Video.",
  "main": "index.js",
  "scripts": {
    "build-ui": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "start": "npx sequelize-cli db:migrate && node index.js",
    "start-dev": "BROWSER=none DISABLE_ESLINT_PLUGIN=true REACT_DEV_SERVER=true TUNNEL=true WATCH=true npm run start",
    "lint": "npx eslint migrations/**/*.js api/**/*.js models/**/*.js receiver/**/*.js index.js",
    "test": "jest",
    "react-test": "react-app-rewired test"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.6",
    "@devicefarmer/adbkit": "^3.2.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/material-next": "^6.0.0-alpha.113",
    "@mui/x-data-grid": "^6.18.5",
    "@mui/x-data-grid-generator": "^6.18.5",
    "@tanstack/react-query": "^4.29.25",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "1.3.5",
    "body-parser": "^1.20.2",
    "connect-ensure-login": "^0.1.1",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jest": "^29.7.0",
    "node-singleflight": "^0.1.2",
    "npm-watch": "^0.11.0",
    "passport": "^0.6.0",
    "passport-trakt": "^1.0.4",
    "promise-streams": "^2.1.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "trakt.tv": "^8.2.0",
    "trakt.tv-images": "^5.2.0",
    "web-vitals": "^3.2.2"
  },
  "devDependencies": {
    "@ngrok/ngrok": "^0.9.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "watch": {
    "build-ui": {
      "silent": true,
      "patterns": [
        "frontend/src/"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "resolvePluginsRelativeTo": "."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
